{
  "name": "angularsignalrclient",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod --output-path=dist/web",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-desktop": "npm run cleanup-desktop && npm run build-prod -- --base-href ./ && npm run desktop-copy-files && npm run pack-desktop",
    "desktop-copy-files": "npm run desktop-copy-app-files && npm run desktop-copy-desktop-files ",
    "desktop-copy-app-files": "copyfiles ./dist/web/**/*.* ./.temp/desktop -f",
    "desktop-copy-desktop-files": "copyfiles ../assets/desktop/**/*.* ./.temp/desktop -f",
    "pack-desktop": "electron-packager .temp/desktop/ --electronVersion=5.0.0 --icon=\".temp/desktop/icon\" --platform=win32,linux --out=./dist/desktop/",
    "cleanup-desktop": "npm run delete-temp-folder && npm run delete-dist-desktop-folder",
    "delete-temp-folder": "rimraf .temp/desktop",
    "delete-dist-desktop-folder": "rimraf dist/desktop"
  },
  "private": true,
  "dependencies": {
    "@microsoft/signalr": "3.0.0",
    "@swimlane/ngx-charts": "^11.1.0",
    "bootstrap": "^4.3.1",
    "electron-packager": "^13.1.1",
    "jquery": "^3.4.1",
    "@angular/animations": "~8.2.5",
    "@angular/common": "~8.2.5",
    "@angular/compiler": "~8.2.5",
    "@angular/core": "~8.2.5",
    "@angular/forms": "~8.2.5",
    "@angular/platform-browser": "~8.2.5",
    "@angular/platform-browser-dynamic": "~8.2.5",
    "@angular/router": "~8.2.5",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.8.2",
    "rimraf": "^2.6.3",
    "copyfiles": "^2.1.0",
    "@angular-devkit/build-angular": "~0.803.4",
    "@angular/cli": "~8.3.4",
    "@angular/compiler-cli": "~8.2.5",
    "@angular/language-service": "~8.2.5",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
