{
  "name": "angularsignalrclient",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod --output-path=dist/web",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-desktop": "npm run cleanup-desktop && npm run build-prod -- --base-href ./ && npm run desktop-copy-files && npm run pack-desktop",
    "desktop-copy-files": "npm run desktop-copy-app-files && npm run desktop-copy-desktop-files ",
    "desktop-copy-app-files": "copyfiles ./dist/web/**/*.* ./.temp/desktop -f",
    "desktop-copy-desktop-files": "copyfiles ../assets/desktop/**/*.* ./.temp/desktop -f",
    "pack-desktop": "electron-packager .temp/desktop/ --electronVersion=5.0.0 --icon=\".temp/desktop/icon\" --platform=win32,linux --out=./dist/desktop/",
    "cleanup-desktop": "npm run delete-temp-folder && npm run delete-dist-desktop-folder",
    "delete-temp-folder": "rimraf .temp/desktop",
    "delete-dist-desktop-folder": "rimraf dist/desktop"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@aspnet/signalr": "^1.1.4",
    "@swimlane/ngx-charts": "^11.1.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "electron-packager": "^13.1.1",
    "jquery": "^3.4.1",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.9",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@fortawesome/fontawesome-free": "^5.8.2",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "rimraf": "^2.6.3",
    "codelyzer": "~4.5.0",
    "copyfiles": "^2.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
