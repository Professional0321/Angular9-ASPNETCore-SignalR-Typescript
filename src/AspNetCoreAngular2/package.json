  "license": "ISC",
  "scripts": {
    "start":
      "concurrently \"webpack-dev-server --env=dev --open\" \"dotnet run\" ",
    "build-dev": "webpack --env=dev",
    "build-production": "webpack --env=prod",
    "prepare-desktop":
      "rimraf .temp/desktop/ && mkdirp ./.temp/desktop && ncp assets/electron .temp/desktop/ && ncp wwwroot .temp/desktop/ && rimraf .dist/desktop/",
    "build-desktop":
      "npm run build-production && npm run prepare-desktop && electron-packager .temp/desktop/ --electronVersion=1.6.5 --platform=win32,linux --out=./.dist/desktop/",
    "build-web":
      "npm run build-production && mkdirp ./.dist/web/ && ncp wwwroot ./.dist/web/",
    "build-all": "npm run build-web && npm run build-desktop"
  },
  "dependencies": {
    "@angular/animations": "5.2.9",
    "@angular/common": "5.2.9",
    "@angular/compiler": "5.2.9",
